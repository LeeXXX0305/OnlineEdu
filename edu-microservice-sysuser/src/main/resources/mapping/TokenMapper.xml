<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onlineEdu.sysuser.mapper.TokenMapper">

    <select id="findByUserId" resultType="com.onlineEdu.sysuser.entity.Token">
        SELECT *
        FROM user_token
        WHERE user_id = #{userId}
    </select>
</mapper>